<?xml version="1.0" encoding="utf-8"?>
<shape
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle"> <!-- 도형의 속성을 : line, oval, rectangle, ring 로 표현 -->
    <solid
        android:color="@color/white"/>
    <stroke
        android:width="1dp"
        android:color="@color/blue"/>
    <size
        android:width="300dp"
        android:height="50dp"/> <!-- 도형의 크기 -->
    <corners
        android:radius="30dp"/>

</shape>